<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="qd_backgroundColor" format="color" />
    <attr name="qd_backgroundColor_disable" format="color" />
    <attr name="qd_borderColor" format="color" />
    <attr name="qd_borderWidth" format="dimension" />
    <attr name="qd_isRadiusAdjustBounds" format="boolean" />
    <attr name="qd_radius" format="dimension" />
    <attr name="qd_radiusTopLeft" format="dimension" />
    <attr name="qd_radiusTopRight" format="dimension" />
    <attr name="qd_radiusBottomLeft" format="dimension" />
    <attr name="qd_radiusBottomRight" format="dimension" />
    <attr name="qd_btn_internal_width" format="dimension" />
    <attr name="qd_textColor" format="color" />
    <attr name="qd_textSize" format="dimension" />

    <attr name="QDButtonStyle" format="reference" />
    <attr name="QDTextViewStyle" format="reference" />
    <declare-styleable name="QDButton2" parent="QDTextView">
        <attr name="qd_backgroundColor" />
        <attr name="qd_backgroundColor_disable" />
        <attr name="qd_borderColor" />
        <attr name="qd_borderWidth" />
        <attr name="qd_isRadiusAdjustBounds" />
        <attr name="qd_radius" />
        <attr name="qd_radiusTopLeft" />
        <attr name="qd_radiusTopRight" />
        <attr name="qd_radiusBottomLeft" />
        <attr name="qd_radiusBottomRight" />
        <attr name="qd_btn_internal_width" />

    </declare-styleable>
    <declare-styleable name="QDTextView2">
        <attr name="qd_backgroundColor" />
        <attr name="qd_borderColor" />
        <attr name="qd_borderWidth" />
        <attr name="qd_isRadiusAdjustBounds" />
        <attr name="qd_radius" />
        <attr name="qd_radiusTopLeft" />
        <attr name="qd_radiusTopRight" />
        <attr name="qd_radiusBottomLeft" />
        <attr name="qd_radiusBottomRight" />
        <attr name="qd_btn_internal_width" />
        <attr name="qd_textColor" />
        <attr name="qd_textSize" />
        <attr name="qd_text" />
    </declare-styleable>
    <declare-styleable name="QDActionBarLayout">
        <attr name="android:layout" />
    </declare-styleable>

    <declare-styleable name="QDViewPager">
        <attr name="canScroll" format="boolean" />
    </declare-styleable>
</resources>