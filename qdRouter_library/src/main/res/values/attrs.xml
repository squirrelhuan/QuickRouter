<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="qd_backgroundColor" format="color" />
    <attr name="qd_backgroundColor_disable" format="color" />
    <attr name="qd_textColor" format="color" />
    <attr name="qd_borderColor" format="color" />
    <attr name="qd_borderWidth" format="dimension" />
    <attr name="qd_isRadiusAdjustBounds" format="boolean" />
    <attr name="qd_radius" format="dimension" />
    <attr name="qd_radiusTopLeft" format="dimension" />
    <attr name="qd_radiusTopRight" format="dimension" />
    <attr name="qd_radiusBottomLeft" format="dimension" />
    <attr name="qd_radiusBottomRight" format="dimension" />
    <attr name="qd_btn_internal_width" format="dimension" />
    <attr name="qd_drawable_width" format="dimension"/>
    <attr name="qd_drawable_height" format="dimension"/>
    <attr name="left_drawable_width" format="dimension"/>
    <attr name="left_drawable_height" format="dimension"/>
    <attr name="right_drawable_width" format="dimension"/>
    <attr name="right_drawable_height" format="dimension"/>
    <attr name="quick_textSize" format="dimension" />
    <attr name="quick_background" format="reference|color" />
    <attr name="quick_checked" format="boolean" />
    <attr name="quick_text" format="string" />
    <attr name="quick_radius" format="dimension" />
    <attr name="quick_square" format="boolean" /><!--是否是方形-->
    <attr name="QDButtonStyle" format="reference" />
    <attr name="QDTextViewStyle" format="reference" />


    <declare-styleable name="ImageTextView" parent="QDTextView">
        <attr name="quick_textSize" />
        <attr name="android:text" />
        <attr name="android:textColor" />
    </declare-styleable>
    <declare-styleable name="QuickView">
        <attr name="quick_radius" />
        <attr name="quick_background" />
        <attr name="quick_checked" />
        <attr name="quick_square" />
    </declare-styleable>
    <declare-styleable name="QDTextView" parent="QuickView">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="qd_backgroundColor" />
        <attr name="qd_borderColor" />
        <attr name="qd_borderWidth" />
        <attr name="qd_isRadiusAdjustBounds" />
        <attr name="qd_radius" />
        <attr name="qd_radiusTopLeft" />
        <attr name="qd_radiusTopRight" />
        <attr name="qd_radiusBottomLeft" />
        <attr name="qd_radiusBottomRight" />
        <attr name="qd_btn_internal_width" />
        <attr name="qd_drawable_width" />
        <attr name="qd_drawable_height" />
        <attr name="left_drawable_width" />
        <attr name="left_drawable_height" />
        <attr name="right_drawable_width" />
        <attr name="right_drawable_height" />
        <attr name="qd_textColor" />
        <attr name="quick_text" />
    </declare-styleable>

    <declare-styleable name="QDActionBarLayout">
        <attr name="android:layout" />
    </declare-styleable>

    <declare-styleable name="QDViewPager">
        <attr name="canScroll" format="boolean" />
    </declare-styleable>
</resources>